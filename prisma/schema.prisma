generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model prompt {
  id         Int       @id @default(autoincrement())
  name       String
  text       String
  createdat  DateTime? @default(now()) @db.Timestamp(6)
  modifiedat DateTime? @default(now()) @db.Timestamp(6)
}
